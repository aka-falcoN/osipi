$Password = Read-Host -AsSecureString
New-ADUser -Name "Sumeet" -GivenName "Sumeet" -Surname "Chaurasia" -SamAccountName "sumeetc" -UserPrincipalName "sumeetc@osipi.quantiphi-sumeet.com" -path "OU=Cloud,DC=osipi, DC=quantiphi-sumeet,DC=com"  -AccountPassword (convertto-securestring $Password -AsPlainText -Force) -enabled $true -ChangePasswordAtLogon $False

################################################### Instance 1 #####################################################3

Informative:
Failover Cluster IP address: 10.0.0.5 (Alias IP)
Availability Group name: cluster-listener
Availability Group Listener address: 10.0.0.6 (Alias IP)


Execution:
$node1 = "mssql-primary-1"
$node2 = "mssql-secondary"
$nameWSFC = "cluster-oispi"
$ipWSFC1 = "10.0.0.5"
$ipWSFC2 = "10.1.0.5"

//optional// netsh interface ip set address name=Ethernet static 10.0.0.4 255.255.0.0 10.0.0.1 1

New-Cluster -Name $nameWSFC -Node $node1, $node2 -NoStorage -StaticAddress $ipWSFC1, $ipWSFC2
Enable-SqlAlwaysOn -ServerInstance $node1 -Force
Enable-SqlAlwaysOn -ServerInstance $node2 -Force

